{
  "name": "youth-empowerment-platform",
  "version": "1.0.0",
  "type": "module",
  "main": "server/src/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" --names \"frontend,backend\" --kill-others",
    "dev:frontend": "cd client && vite",
    "dev:backend": "nodemon server/src/index.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd client && vite build",
    "build:backend": "echo 'Backend build not required for Node.js'",
    "start": "node server/src/index.js",
    "lint": "eslint client/src --ext .ts,.tsx",
    "create-admin": "node create_admin.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@paypal/react-paypal-js": "^8.9.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.0",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/react-query": "^4.36.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.16.4",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.284.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.69.0",
    "stripe": "^13.10.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-pwa": "^1.2.0",
    "wouter": "^2.11.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^7.3.0"
  }
}
